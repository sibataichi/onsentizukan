<h2>投稿編集</h2>
<%= form_with model: @post do |f| %>
<p>
  <label for="inputImage">投稿画像</label>
  <%= f.file_field :image, placeholder: "投稿画像", accept: "image/*" %>
</p>
<p>
  <label for="inputTitle">タイトル</label>
  <%= f.text_field :title, autofocus: true, id:"inputTitle", placeholder:"タイトル"%>
</p>
<P>
  <label for="inputBody">説明</label>
  <%= f.text_field :body, autofocus: true, id:"inputBody", placeholder:"説明"%>
</P>
  <%= f.submit "変更を保存" %>
<% end %>