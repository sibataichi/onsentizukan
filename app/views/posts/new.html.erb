<h1>画像投稿フォーム</h1>
  <%= form_with model: @post do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>説明</h4>
      <%= f.text_area :body %>


    <div class="form-group">
     <%= f.label :address, '住所' %>
     <%= f.text_field :address, class: 'form-control' %>
    </div>

    <%= f.submit '投稿' %>
  <% end %>